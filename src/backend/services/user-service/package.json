{
    "name": "user-service",
    "version": "2.0.0",
    "description": "User Service - Authentication API with OTP, Profile, Address Management",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "dev": "node index.js",
        "db:migrate": "npx sequelize-cli db:migrate",
        "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
        "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
        "db:seed": "npx sequelize-cli db:seed:all",
        "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
        "db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
        "db:setup": "npx sequelize-cli db:create && npm run db:migrate && npm run db:seed"
    },
    "keywords": [
        "user",
        "authentication",
        "jwt",
        "otp",
        "express",
        "sequelize"
    ],
    "author": "Yummy Team",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.1",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "mysql2": "^3.6.5",
        "sequelize": "^6.35.2"
    },
    "devDependencies": {
        "sequelize-cli": "^6.6.5"
    }
}